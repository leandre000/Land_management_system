# Application
PORT=3000
NODE_ENV=development

# Database - use Docker service names
DB_HOST=db                # Changed from 'localhost'
DB_PORT=5432              # Same (container port)
DB_USERNAME=postgres
DB_PASSWORD=postgres      # Must match docker-compose.yml
DB_NAME=land_management

# JWT
JWT_SECRET=your-super-secret-key-here
JWT_EXPIRES_IN=1d

# Redis - use Docker service name
REDIS_HOST=redis          # Changed from 'localhost'
REDIS_PORT=6379

# RabbitMQ - use Docker service name
RABBITMQ_URL=amqp://rabbitmq:5672  # Changed from 'localhost'